<?xml version="1.0"?>
<block>
  <name>add_message_to_stream_async</name>
  <key>echo_add_message_to_stream_async</key>
  <category>[echo]</category>
  <import>import echo</import>
  <make>echo.add_message_to_stream_async()</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <!-- <param>
    <name>...</name>
    <key>...</key>
    <type>...</type>
  </param> -->

  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>
  <sink>
    <name>msgs</name>
    <type>message</type>
  </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
